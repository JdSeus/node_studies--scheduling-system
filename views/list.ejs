<h1>Consultas</h1>
<hr>
<form action="" method="post">
    <input type="search" name="search" id="search" placeholder="Digite o e-mail ou o cpf">
    <button>Pesquisar</button>
</form>
<hr>
<% appos.forEach(appo => { %>
    <h1>Consulta: <%= appo.description %></h1>
    <h4>Paciente: <%= appo.name %></h4>
    <h4>Email: <%= appo.email %></h4>
    <h4>CPF: <%= appo.cpf %></h4>
    <hr>
<% }) %>